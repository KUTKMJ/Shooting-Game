}

void Boss_Mod1_Form(char boss_mod1_Form[50][80]) {
	int x, y, Boss_Mod1_Location,location_x,location_y;
	char Life, life;
	srand((unsigned)time(NULL));
	Life = 57;
	life = 57;
	for (x=0;x<50;x++){
		for (y = 0; y < 80; y++) {
			for (;;) {
				location_x = rand() % 30 + 1;
				location_y = rand() % 50 + 1;
				boss_mod1_Form[location_x][location_y] = 57;
				boss_mod1_Form[location_x][location_y + 1] = 57;
				boss_mod1_Form[location_x][location_y + 2] = 196;
				boss_mod1_Form[location_x][location_y - 1] = 196;
				boss_mod1_Form[location_x][location_y - 2] = 192;
				boss_mod1_Form[location_x][location_y + 3] = 217;
				boss_mod1_Form[location_x - 1][location_y + 3] = 179;
				boss_mod1_Form[location_x - 1][location_y - 1] = 179;
				boss_mod1_Form[location_x - 2][location_y + 3] = 191;
				boss_mod1_Form[location_x - 2][location_y - 1] = 218;
				boss_mod1_Form[location_x - 2][location_y] = 196;
				boss_mod1_Form[location_x - 2][location_y + 1] = 196;
				boss_mod1_Form[location_x - 2][location_y + 2] = 196;
				if (myBullet_Location[x][y] == boss_mod1_Form[location_x][location_y] || myBullet_Location[x][y] == boss_mod1_Form[location_x][location_y+1]) {
					if (life != 48) life--;
					else if (life == 48) {
						Life--;
						life = 57;
					}
				}
				if (x > 50 || y > 80 || x < 0 || y < 0 || Life == 48) {
					return;
					break;
				}
			}
		}
	}

}




